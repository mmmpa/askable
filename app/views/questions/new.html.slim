= stylesheet_link_tag '/css/codemirror.css', media: 'all'
div#new-question

script src='/js/new-question/built.js'
javascript:
  var params = {
    user: #{@user.to_json.html_safe},
    group: #{@group.to_json.html_safe},
    groupId: #{@group.id},
    questionPage: '#{question_path(question_id: ":questionId")}'
  };
  NewQuestion.start(document.querySelector('#new-question'), params);